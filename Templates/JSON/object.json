
{
  "name": "object",                   //имя объекта
  "properties":{
    "coordinate": ["x","y"],          //координаты объекта
    "movement":{
      "is_velocity": false,           //двигается ли объект
      "direction": ["up", "down", "left", "right"], //направление движения
      "speed": ["speed_x", "speed_y"],//начальная скорость
    },
    "hit_points": 15,                 //количество hp, необходимое, для разрушения объект
    "size": ["size_x", "size_y"],     //размер объекта
    "solid": true,                    //материальность объекта
    "is_on_floor": true,              //проверка нахождения объекта на земле
    "visible":{
      "is_visible": true,             //невидимость объекта
      "percent": 50,                  //прозрачность
    },
    "is_interactive": false,          //можно ли взаимодействовать с объектом (разговаривать)
    "is_touched": false,              //соприкасается ли с другими объектами
    "alive":{
      "is_alive": true,               //проверка, не уничтожен ли объект
      "texture": ["alive", "dead"],   //разные текстуры для живого и мертвого объекта
      "animation": ["alive", "dead"], //разные анимации для живого и мертвого объекта
    },
    "is_static": true,                //можно ли двигать объект    
    "contains":{
      "is_contains": false,           //хранит ли в себе какие-либо бонусы (напитки, оружие и прочие ништяки) или может ли дать квест
      "content": ["weapon","drinks","other"], //содержимое объекта
    },
  },
  "animation":{
    "is_animated": false,             //анимирован ли объект
    "texture": "texture",             //текстура объекта
    "anim_path": "pictures_for_anim", //путь к кадрам анимации
  },
}






}
